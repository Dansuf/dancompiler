VAR
 a[100]
BEGIN
  READ a[0];
  READ a[1];
  READ a[2];
  READ a[3];
  READ a[4];
  READ a[5];

  IF a[0] >= a[1] THEN
    WRITE 0;
  ELSE
    WRITE 1;
  ENDIF
  IF a[1] <= a[2] THEN
    WRITE 2;
  ELSE
    WRITE 3;
  ENDIF
  IF a[2] > a[3] THEN
    WRITE 4;
  ELSE
    WRITE 5;
  ENDIF
  IF a[3] < a[4] THEN
    WRITE 6;
  ELSE
    WRITE 7;
  ENDIF
  IF a[4] = a[5] THEN
    WRITE 8;
  ELSE
    WRITE 9;
  ENDIF
  IF a[5] <> a[0] THEN
    WRITE 10;
  ELSE
    WRITE 11;
  ENDIF
END
